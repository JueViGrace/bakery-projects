findUsers:
SELECT *
FROM user;

findUser:
SELECT *
FROM user
WHERE id = :id;

insert:
INSERT OR REPLACE INTO user(
    id,
    first_name,
    last_name,
    username,
    email,
    phone_number,
    birth_date,
    address1,
    address2,
    gender,
    created_at
) VALUES ?;

delete:
DELETE FROM user
WHERE id = :id;
