---
import StoreLayout from '@layouts/StoreLayout.astro';

interface Props {
  title: string;
}

const { title } = Astro.props;

export const prerender = true;
---

<StoreLayout title={title}>
  <main
    class="container flex flex-col items-center justify-center gap-8 px-4 py-24"
  >
    <slot />
  </main>
</StoreLayout>
