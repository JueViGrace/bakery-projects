{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@layouts/*": [
        "./src/layouts/*"
      ],
      "@ui/*": [
        "./src/components/*"
      ],
      "@auth/*": [
        "./src/features/auth/*"
      ],
      "@cart/*": [
        "./src/features/cart/*"
      ],
      "@profile/*": [
        "./src/features/profile/*"
      ]
    }
  }
}
