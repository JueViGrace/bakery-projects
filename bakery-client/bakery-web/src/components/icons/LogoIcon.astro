---
import { Image } from 'astro:assets';
import ChefImg from '../../assets/chef.png';
import { cn } from '@/lib/utils';

interface Props {
  className?: string;
}

const { className } = Astro.props;

export const prerender = true;
---

<Image
  loading="eager"
  class={cn('size-fit', className)}
  src={ChefImg}
  alt="logo"
/>
